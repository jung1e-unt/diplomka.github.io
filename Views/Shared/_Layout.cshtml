<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BeeGroup</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/BeeGroup.styles.css" asp-append-version="true" />
    @await Html.PartialAsync("CssPartical")
</head>
<body>
    <div id="page-wrapper">
        @await Html.PartialAsync("HeaderPartical")
        <div id="main">
            <div class="container">
                <main role="main" class="pb-3">
            @RenderBody()
                </main>
            </div>
        </div>
    </div>


</body>
</html>
